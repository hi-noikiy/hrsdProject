//头部
.head{
    color: #9bacbd;
    font-size: 2.75rem;
    text-align: center;
    border-bottom: 1px solid #495057;
    line-height: 66px;
    padding-top: 45px;
    background: #252732;
    a{
        margin: 0 35px;
        display: inline-block;
        &:focus{
            color: #fff;
        }
    }
    
    .active{
        border-bottom: 4px solid #17a7f3;
        color: #fff;
    }
}